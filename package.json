{
  "name": "@dotnames/dotnames-js",
  "version": "0.0.5",
  "description": "A Universal SDK to resolve names from all blockchains",
  "keywords": [
    "dotnames-sdk",
    "dotnames",
    "dotlabs",
    "universal-sdk",
    "sdk",
    "ENS",
    "Space-Id",
    "UD",
    "SNS",
    "SuiNs",
    "AptosNs"
  ],
  "author": "dotlabs",
  "license": "MIT",
  "scripts": {
    "ts-types": "tsc",
    "build:prod": "rimraf dist && tsup",
    "build:watch": "tsup --watch",
    "lint:fix": "eslint ./src --ext .ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix "
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.6.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tsup": "^6.0.1"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@bonfida/spl-name-service": "^1.0.6",
    "@cosmjs/cosmwasm-stargate": "^0.28.11",
    "@dotbit/plugin-avatar": "^0.0.1",
    "@dotnames/dotseijs": "^0.0.3",
    "@ensdomains/ensjs": "^3.0.0-alpha.65",
    "@mysten/sui.js": "^0.37.1",
    "@sei-js/core": "^3.1.3",
    "@siddomains/sei-sidjs": "^0.0.4",
    "@siddomains/sidjs": "^0.1.29",
    "@solana/web3.js": "^1.78.0",
    "@unstoppabledomains/resolution": "^9.1.0",
    "dotbit": "^0.4.22",
    "ethers": "^5.7.2",
    "sgnames.js": "^0.1.1",
    "typescript": "^4.7.2",
    "zksync-ethers": "^5.0.0"
  },
  "files": [
    "dist",
    "package.json"
  ]
}
